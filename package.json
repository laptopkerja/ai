{
  "name": "ai-content-generator",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "vite",
    "server": "nodemon server/index.js --watch server --exec node",
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "test": "node --test tests",
    "test:unit": "node --test tests/unit",
    "test:integration": "node --test tests/integration",
    "smoke:new-platforms": "node scripts/smoke-new-platform-presets.mjs",
    "env:status": "node scripts/switch-env.mjs status",
    "env:use:primary": "node scripts/switch-env.mjs primary",
    "env:use:backup": "node scripts/switch-env.mjs backup",
    "check:syntax": "node --check server/index.js && node --check server/admin-auth.js && node --check server/lib/aiProviders.js && node --check server/lib/providerKeysCrypto.js && node --check server/lib/providerModelDiscovery.js && node --check server/lib/generationQuality.js",
    "quality": "npm run check:syntax && npm run test && npm run build && node scripts/validate-presets.mjs"
  },
  "dependencies": {
    "@iconify/react": "^6.0.2",
    "@supabase/supabase-js": "^2.0.0",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-router-dom": "^6.14.1",
    "uuid": "^9.0.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^8.2.0",
    "dotenv": "^17.3.1",
    "nodemon": "^3.0.1",
    "vite": "^5.3.0"
  }
}
